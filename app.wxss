/**app.wxss**/
page{background:#f9f9f9;font-size:26rpx;color: #1b1b1b;}
image{vertical-align: middle;}
.text-center{text-align: center;}
.pull-left{float: left;}
.pull-right{float: right;}
.clearfix{clear: both;}

.bg-fff{background-color:#fff;}
.mt-10{margin-top: 10rpx;}
.mt-20{margin-top: 20rpx;}
.p-20{padding: 20rpx;}
.text-red{color: red;}
.row{width: 100%;}

.on{border-bottom:2px solid red;color:red;}


.toast{position: fixed;bottom: 15%;width: 100%;z-index: 1111;text-align: center;color:#fff;}
.toast .toast-mask{display:inline-block;background-color: #000;width: 30%;padding: 20rpx;border-radius: 10rpx;}
.loading image{width: 100prx;height: 100rpx;}
.loading .no-data{padding: 40rpx;color: #5d5d5d;font-size: 28rpx;}
::-webkit-scrollbar{
  width:0;
  height:0;
  color:transparent;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
 /*总体主盒子*/  
 .container_search {  
  position: relative;  
  width: 100%;    
  background-color: #fff;  
  color: #939393;  
}
 /*顶部搜索栏*/
.search_btn{
  padding:7px 10px; 
  background-color:#f2f2f2;
  border-top:1px solid #d8d8d8;
  border-bottom:1px solid #d8d8d8 ;
  width:100%;
  box-sizing:border-box;
}
.search_btn view{
  height:30px;
  background-color:#fff;
  text-align:center;
  line-height:30px;
  border-radius:5rpx;
}
.search_btn view image{
  width:30rpx;
  height:30rpx;
  margin-top:-5rpx;
  display:inline-block ;
}
.friendBox{
  padding: 30rpx;
  background: #fff;
  border-bottom: 1px solid #dcdcdc;
  display: flex;
  justify-content: space-between;
}
.friendBox .friendPortial{
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
}
.friendTextBox{
  flex:1;
  margin-left: 20rpx;
}
.friendTextBox view{
  margin-bottom: 10rpx;
}
.usedhuidou{
  color: crimson;
}

